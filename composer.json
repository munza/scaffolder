{
    "name": "Munza/Scaffolder",
    "type": "library",
    "description": "A generator for generating generators for Laravel and Lumen.",
    "keywords": [
        "Munza",
        "Scaffolder"
    ],
    "homepage": "https://github.com/Munza/Scaffolder",
    "license": "MIT",
    "authors": [
        {
            "name": "Tawsif Aqib",
            "email": "tawsif.aqib@gmail.com",
            "homepage": "https://munza.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Munza\\Scaffolder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Munza\\Scaffolder\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
